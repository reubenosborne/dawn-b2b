{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "text": "Subtitle",
            "text_style": "subtitle"
          }
        },
        "fbe531b3-6232-4448-9281-7748c4f830ce": {
          "type": "text",
          "settings": {
            "text": "Dropping on {{ product.metafields.my_fields.release_date | metafield_text }}",
            "text_style": "uppercase"
          }
        },
        "price": {
          "type": "price",
          "disabled": true,
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "disabled": true,
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "disabled": true,
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "disabled": true,
          "settings": {
            "show_dynamic_checkout": false
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "share": {
          "type": "share",
          "settings": {
          }
        },
        "9d682a9b-a24e-4ec1-a447-9f09a85ab166": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Specifications",
            "content": "<p>{{ product.metafields.my_fields.carats.value }} Carats<\/p>",
            "page": "",
            "icon": "star"
          }
        },
        "0791aad5-0678-4977-90ad-65a32fb28ac2": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Dimensions",
            "content": "<p><strong>Length: <\/strong>{{ product.metafields.my_fields.length | metafield_tag }}<br\/><strong>Width: <\/strong>{{ product.metafields.my_fields.width | metafield_tag }}<br\/><strong>Depth: <\/strong>{{ product.metafields.my_fields.height | metafield_tag }}<\/p>",
            "page": "",
            "icon": "ruler"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "subtitle",
        "fbe531b3-6232-4448-9281-7748c4f830ce",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "share",
        "9d682a9b-a24e-4ec1-a447-9f09a85ab166",
        "0791aad5-0678-4977-90ad-65a32fb28ac2"
      ],
      "settings": {
        "enable_sticky_info": true,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "image_ratio": "adapt",
        "show_secondary_image": false,
        "add_image_padding": false,
        "show_vendor": false
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
